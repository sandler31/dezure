{
  "name": "Dezure",
  "description": "Bypasses the annoying step on azure resource deletion",
  "version": "1.0",
  "permissions": [
    "activeTab"
  ],
  "content_scripts": [
    {
    "matches": [
          "*://portal.azure.com/*"
        ],
    "js": ["content.js"],
    "all_frames": true,
    "run_at": "document_end"
    }
  ],
  "manifest_version": 2
}
